@@ -158,20 +158,48 @@ static void _mxc_serial_init(struct mxc_uart *base, int use_dte)
 	writel(0, &base->ts);
 }
 
+static unsigned long mxc_serial_get_rfdiv_reg_val(unsigned long rfdiv)
+{
+    switch (rfdiv) {
+    case 1: return 5;
+    case 2: return 4;
+    case 3: return 3;
+    case 4: return 2;
+    case 5: return 1;
+    case 6: return 0;
+    case 7: return 6;
+    default:
+        return 0;
+    }
+}
+
+#define MAX(a,b) (((a)>(b))?(a):(b))
+
 static void _mxc_serial_setbrg(struct mxc_uart *base, unsigned long clk,
 			       unsigned long baudrate, bool use_dte)
 {
 	u32 tmp;
-
-	tmp = RFDIV << UFCR_RFDIV_SHF;
-	if (use_dte)
+	u32 rfdiv;
+
+	rfdiv = clk / (16 * baudrate);
+	if (rfdiv > 7) {
+		rfdiv = 7;
+    }
+	tmp = mxc_serial_get_rfdiv_reg_val(rfdiv) << UFCR_RFDIV_SHF;
+	if (use_dte) {
 		tmp |= UFCR_DCEDTE;
-	else
-		tmp |= (TXTL << UFCR_TXTL_SHF) | (RXTL << UFCR_RXTL_SHF);
+	}
+	tmp |= (TXTL << UFCR_TXTL_SHF) | (RXTL << UFCR_RXTL_SHF);
 	writel(tmp, &base->fcr);
 
-	writel(0xf, &base->bir);
-	writel(clk / (2 * baudrate), &base->bmr);
+	unsigned long num = clk;
+	unsigned long denom = 16 * rfdiv * baudrate;
+	unsigned long scale = (MAX(num, denom) + 65535) / 65536;
+	unsigned long ubmrPlusOne = num / scale;
+	unsigned long ubirPlusOne = denom / scale;
+
+	writel(ubirPlusOne - 1, &base->bir);
+	writel(ubmrPlusOne - 1, &base->bmr);
 
 	writel(UCR2_WS | UCR2_IRTS | UCR2_RXEN | UCR2_TXEN | UCR2_SRST,
 	       &base->cr2);
